{
  "name": "express-mongoose-es6-rest-api",
  "version": "2.0.0",
  "description": "A Boilerplate application for building REST APIs using express, mongoose in ES6 with code coverage",
  "author": "Kunal Kapadia <kunalkapadia12@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=app:* yarn start",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover './node_modules/mocha/bin/_mocha' -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "publish:staging": "fly staging",
    "publish:production": "fly production"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/khaleel/supplieson_backend.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.1.4",
    "async": "^2.5.0",
    "barc": "0.0.4",
    "barcode-generator": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "canvas": "^2.8.0",
    "compression": "1.6.2",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-fileupload": "0.3.0",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "faker": "4.1.0",
    "fibers": "^3.1.1",
    "file-type": "^7.4.0",
    "flightplan": "^0.6.17",
    "gulp": "^4.0.2",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "10.0.6",
    "jsbarcode": "^3.11.0",
    "jsonwebtoken": "7.1.9",
    "jsreport": "1.10.0",
    "jsreport-core": "^1.5.1",
    "jsreport-express": "^1.2.4",
    "jsreport-html-to-xlsx": "^1.4.6",
    "jsreport-jsrender": "^1.0.2",
    "jsreport-phantom-pdf": "^1.4.6",
    "jsreport-templates": "^1.2.2",
    "jsreport-xlsx": "^1.4.2",
    "method-override": "^2.3.5",
    "moment": "2.19.3",
    "moment-hijri": "2.1.1",
    "moment-timezone": "0.5.21",
    "mongoose": "4.7.4",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-sequence-plugin": "^1.0.5",
    "morgan": "1.7.0",
    "multer": "^1.3.0",
    "node-cron": "^1.2.1",
    "node-uuid": "1.4.8",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "phantomjs": "^2.1.7",
    "qrcode": "^1.4.4",
    "request": "^2.86.0",
    "sendgrid": "^5.2.3",
    "swagger-ui-express": "^4.1.4",
    "validator": "^9.0.0",
    "winston": "^2.4.0",
    "winston-mongodb": "^3.0.1",
    "xlsx": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "flightplan": "^0.6.17",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "^0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "jsreport-core": "^1.5.1",
    "jsreport-express": "^1.2.4",
    "jsreport-html-to-xlsx": "^1.4.6",
    "jsreport-jsrender": "^1.0.2",
    "jsreport-phantom-pdf": "^1.4.6",
    "jsreport-templates": "^1.2.2",
    "jsreport-xlsx": "^1.4.2",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
